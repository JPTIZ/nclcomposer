#
# QNLYROOT = \
#    <PATH>: Project root location.
#

QNLYROOT = \
    $$PWD

#
# QNLYBIN = \
#    <PATH>: Default binary files location.
#

QNLYBIN = \
    $$QNLYROOT/bin

#
# QNLYLIB = \
#    <PATH>: Default library files location.
#

QNLYLIB = \
    $$QNLYROOT/lib

#
# QNLYCONFIG = \
#    standalone: Compile project as a standalone application.
#    plugin: Compile project as a plugin application.
#

QNLYCONFIG = \
    plugin
